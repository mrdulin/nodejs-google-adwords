{
  "name": "nodejs-google-adwords",
  "version": "1.3.3",
  "description": "Google Ads API Client Library for Node.js",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "prettify": "prettier --config ./.prettierrc --write \"./{src,__{tests,mocks}__}/**/*.{ts,js}\"",
    "prettify-check": "prettier --check \"./{src,__{tests,mocks}__}/**/*.{ts,js}\"",
    "prepare": "npm run lint && npm run build"
  },
  "keywords": [
    "Google Ads",
    "Google Adwords",
    "Node.js"
  ],
  "author": "mrdulin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:mrdulin/nodejs-google-adwords.git"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.7",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/xml2js": "^0.4.4",
    "dotenv": "^6.2.0",
    "faker": "^4.1.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "pretty-data": "^0.40.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "soap": "^0.27.0",
    "xml2js": "^0.4.19"
  },
  "engines": {
    "node": ">=8.11.4"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run build && npm run prettify"
    }
  }
}
